<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>HttpServletResponsex</servlet-name>
    <servlet-class>com.jiangsu.httpServletResponse.HttpServletResponsex</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>DownloadTest</servlet-name>
    <servlet-class>com.jiangsu.httpServletResponse.DownloadTest</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ServletDownloadTest</servlet-name>
    <servlet-class>com.jiangsu.httpServletResponse.ServletDownloadTest</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ServletDemo1</servlet-name>
    <servlet-class>com.jiangsu.httpServletResponse.ServletDemo1</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>Login</servlet-name>
    <servlet-class>com.jiangsu.httpServletResponse.Login</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>SkipPage</servlet-name>
    <servlet-class>com.jiangsu.httpServletResponse.SkipPage</servlet-class>
  </servlet>






  <servlet-mapping>
    <servlet-name>HttpServletResponsex</servlet-name>
    <url-pattern>/servlet/HttpServletResponsex</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DownloadTest</servlet-name>
    <url-pattern>/servlet/DownloadTest</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServletDownloadTest</servlet-name>
    <url-pattern>/servlet/ServletDownloadTest</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServletDemo1</servlet-name>
    <url-pattern>/servlet/demo1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/servlet/Login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SkipPage</servlet-name>
    <url-pattern>/servlet/SkipPage</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
